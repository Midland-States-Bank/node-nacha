{
  "name": "@midlandsbank/node-nacha",
  "version": "2.0.0",
  "type": "module",
  "description": "NACHA ACH EFT File Parser/Formatter for CCD+ / PPD+ / CTX+",
  "keywords": [
    "eft",
    "ach",
    "nacha",
    "ccd",
    "ccd+",
    "ppd",
    "ppd+",
    "parse",
    "parser",
    "format",
    "formatter",
    "reader",
    "writer",
    "json"
  ],
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --sourcemap --watch",
    "test": "vitest"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  }
}
