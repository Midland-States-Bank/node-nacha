{
  "name": "@msb/node-nacha",
  "version": "1.1.0",
  "description": "NACHA ACH EFT File Parser/Formatter for CCD+ / PPD+ / CTX+",
  "keywords": [
    "eft",
    "ach",
    "nacha",
    "ccd",
    "ccd+",
    "ppd",
    "ppd+",
    "parse",
    "parser",
    "format",
    "formatter",
    "reader",
    "writer",
    "json"
  ],
  "main": "lib",
  "bin": "bin/ach.js",
  "scripts": {
    "compile": "coffee --bare --compile lib bin",
    "clean": "rm -f ./bin/command.js ; find ./lib -name \"*.js\" -type f -delete",
    "prepublish": "npm run compile",
    "postpublish": "npm run clean",
    "test": "jest"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "dependencies": {
    "each-part": "^0.9.0",
    "requireg": "^0.1.6",
    "strung": "^1.0.5"
  }
}
